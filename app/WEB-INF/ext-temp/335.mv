select a3.ID a3_ID,a3.NAME a3_NAME,sum(a0.JE) sum_1  from MX_DATAS_ar a0 join DEPT a1 on a0.JGID=a1.DEPT_ID  left join CODE_AREA a2 on a0.AREA=a2.ID  join CODE_INCOME a3 on a0.INCOME=a3.ID  where 1=1  and a1.lv2_id = '0010102'  and a0.RQ in ('2013-04-01') and a2.ID in (7) and a1.DEPT_ID in ('001010205') group by a3.ID,a3.NAME