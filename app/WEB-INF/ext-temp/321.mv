select sum(a0.JE) sum_1,a1.DEPT_ID as a1_DEPT_ID,a1.DEPT_NAME as a1_DEPT_NAME,a2.ID as a2_ID,a2.NAME as a2_NAME,a0.RQ as a0_RQ,a3.ID as a3_ID,a3.NAME as a3_NAME  from MX_DATAS_ar a0 join DEPT a1 on a0.JGID=a1.DEPT_ID  left join CODE_AREA a2 on a0.AREA=a2.ID  join CODE_INCOME a3 on a0.INCOME=a3.ID  where 1=1  and a1.lv2_id = '0010102'  and a2.ID in (3) and a0.RQ in ('2013-01-05') group by a1.DEPT_ID,a1.DEPT_NAME,a2.ID,a2.NAME,a0.RQ,a3.ID,a3.NAME